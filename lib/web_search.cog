flow web_search(query: String, num_results: Int = 5) -> String:
    "Search the web using DuckDuckGo and return titles + summaries"
    return __exec_shell__(f"/home/reza/.local/bin/ddgr --json -n {num_results} \"{query}\" 2>/dev/null | python3 -c \"import sys,json; [print(r['title']+'\\n'+r['abstract']+'\\n'+r['url']+'\\n') for r in json.load(sys.stdin)]\" 2>/dev/null | head -100")
